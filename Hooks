import './App.css';
import React, { useState } from 'react';

const MyComponent = () => {
  const [count, setCount] = useState(0);

  return (
    <div>
      <h1>Counter is {count}</h1> 
      <button onClick={() => setCount(count + 1)}>Click me</button> {/* Pass a function to onClick */}
    </div>
  );
};

function App() {
  return (
    <div>
      <MyComponent />
    </div>
  );
}

export default App;
